#!/bin/bash

if [[ ! -x `which brew` ]]; then
  /usr/bin/ruby -e "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/master/install)"
fi

brew analytics off

brew upgrade

brew install vim
brew install tmux
brew install ctags
brew install the_silver_searcher
brew install reattach-to-user-namespace
brew install fzf

brew install fish

brew install rbenv

brew cleanup
